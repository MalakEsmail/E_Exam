        <activity android:name=".Student.studentLoginActivity" />
        <activity android:name=".Student.studentSubjectsActivity" />
        <activity android:name=".Student.student_sign_up_Activity" />
        <activity
            android:name=".Professor.subjectActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Professor.ProfessorActivity" />
        <activity
            android:name=".Admin.AdminActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>